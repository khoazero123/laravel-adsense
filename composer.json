{
    "name": "martinbutt/laravel-adsense",
    "description": "Display Adsense ads easily in Laravel 6.x, 7.x and 8.x",
    "keywords": [ "laravel", "adsense", "ads" ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Butt",
            "homepage": "https://www.martinbutt.com",
            "role": "Developer"
        },
        {
            "name": "Crypto Technology srl",
            "email": "admin@cryptotech.srl",
            "homepage": "https://cryptotech.srl",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/martinbutt/laravel-adsense/issues",
        "source": "https://github.com/martinbutt/laravel-adsense/"
    },
    "require": {
        "php": "^7.2||^8.0",
        "illuminate/view": "^6.0||^7.0||^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "MartinButt\\Laravel\\Adsense\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MartinButt\\Laravel\\Adsense\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MartinButt\\Laravel\\Adsense\\Providers\\AdsenseServiceProvider"
            ],
            "aliases": {
                "Adsense": "MartinButt\\Laravel\\Adsense\\Facades\\AdsenseFacade"
            }
        }
    },
    "scripts": {
        "test": "composer dump-autoload && phpunit --configuration phpunit.xml",
        "php-cs-fixer": "php-cs-fixer fix . --verbose --show-progress=estimating --dry-run --diff"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
